#{param name=missions/}

<ul>
    <% missions.each { mission -> %>
        <li>
            ${mission.getTitle()}
            <%
            mid =mission.getId();
            %>
                <a href="@{JuZBackEndApplication.editMission(id=mid)}" role="button">edit a missions</a>
        </li>
    <% } %>
</ul>
<a href="@{JuZBackEndApplication.index()}" role="button">Home</a>
